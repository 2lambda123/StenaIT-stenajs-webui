{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "rimraf build && build-storybook -o build",
    "watch-storybook": "concurrently \"npm:start-storybook\" \"npm:watch\"",
    "deploy-storybook": "yarn build && yarn build-storybook && yarn deploy-built-storybook",
    "deploy-built-storybook": "gh-pages -d build",
    "build": "lerna run build",
    "watch": "lerna exec --parallel -- rollup -c -w",
    "prerelease": "yarn build && yarn test && yarn check-deps-match",
    "test": "jest --config=jest.config.js",
    "check-deps-match": "node check-dep-versions.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StenaIT/stenajs-webui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StenaIT/stenajs-webui/issues"
  },
  "homepage": "https://github.com/StenaIT/stenajs-webui#readme",
  "devDependencies": {
    "@dump247/storybook-state": "^1.5.2",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@storybook/theming": "^5.0.11",
    "@svgr/rollup": "^4.2.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.0",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react": "^4.0.1",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "gh-pages": "^2.0.1",
    "jest": "^24.8.0",
    "lerna": "^3.13.4",
    "prettier": "^1.17.0",
    "react-docgen-typescript-loader": "^3.1.0",
    "rollup": "^1.11.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.21.0",
    "rollup-plugin-url": "^2.2.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  }
}
